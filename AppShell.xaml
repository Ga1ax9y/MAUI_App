<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Stanishewski253505.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Stanishewski253505"
    Shell.FlyoutBehavior="Flyout"
    Title="Stanishewski253505">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent
        Title="Calculator"
        ContentTemplate="{DataTemplate local:CalculatorPage}"
        Route="CalculatorPage" />
    <ShellContent
    Title="ProgressBar"
    ContentTemplate="{DataTemplate local:ProgressPage}"
    Route="ProgressPage" />
    
</Shell>
